{
  "default": {
    "require": [
      "src/test/stepDefinitions/*.ts",
      "src/test/hooks/*.ts"
    ],
    "paths": ["src/test/features/*.feature"],
    "requireModule": ["ts-node/register"],
    "format": [
      "json:report-results/report.json",
      "rerun: @rerun.txt"
    ],
    "publishQuiet": true
  },
  "rerun": {
    "require": [
      "src/test/stepDefinitions/*.ts",
      "src/test/hooks/*.ts"
    ],
    "requireModule": ["ts-node/register"],
    "format": [
      "json:report-results/report.json",
      "rerun: @rerun.txt"
    ],
    "publishQuiet": true
  }
}
